issn = {15493636},
journal = {Journal of Computer Science},
keywords = {Artificial Neural Networks (ANN),Error reduction,Learning algorithms,Time series forecasting},
mendeley-groups = {TESIS/RESULTADOS/Google Acad{\'{e}}mico},
number = {7},
