pages = {222--236},
publisher = {Springer Science and Business Media Deutschland GmbH},
title = {{Predictive Academic Performance Model to Support, Prevent and Decrease the University Dropout Rate}},
volume = {1455 CCIS},
year = {2021}
}
@article{Xiao2021,
abstract = {Recent advancements in Artificial Intelligence techniques, including machine learning models, have led to the expansion of prevailing and practical prediction simulations for various fields. The quality of teachers' performance mainly influences the quality of educational services in universities. One of the major challenges of higher education institutions is the increase of data and how to utilize them to enhance the academic program's quality and administrative decisions. Hence, in this paper, Artificial Intelligence assisted Multi-Objective Decision-Making model (AI-MODM) has been proposed to predict the instructor's performance in the higher education systems. The proposed AI-assisted prediction model analyzes the numerical values on various elements allocated for a cluster of teachers to evaluate an overall quality evaluation representing the individual instructor's performance level. Instead of replacing teachers, AI technologies would increase and motivate them. These technologies would reduce the time necessary for routine tasks to enable the faculty to focus on teaching and analysis. The usage for administrative decision-making of artificial intelligence and associated digital tools. The experimental results show that the suggested AI-MODM method enhances the accuracy (93.4{\%}), instructor performance analysis (96.7{\%}), specificity analysis (92.5{\%}), RMSE (28.1 {\%}), and precision ratio (97.9{\%}) compared to other existing methods.},
author = {Xiao, Shuping and Shanthini, A. and Thilak, Deepa},
doi = {10.1142/S0219265921440035},
issn = {17936713},
journal = {Journal of Interconnection Networks},
keywords = {Artificial intelligence,higher education system,instructor performance,prediction model},
mendeley-groups = {TESIS/ARTICULOS/Scopus},
publisher = {World Scientific},
title = {{Instructor Performance Prediction Model Using Artificial Intelligence for Higher Education Systems}},
year = {2021}
}
@article{Chamorro-Atalaya2021,
abstract = {In this competitive scenario of the educational system, higher education institutions use intelligent learning tools and techniques to predict the factors of student academic performance. Given this, the article aims to determine the supervised learning model for the predictive system of personal and social attitudes of university students of professional engineering careers. For this, the Machine Learning Classification Learner technique is used by means of the Matlab R2021a software. The results reflect a predictive system capable of classifying the four satisfaction classes (1: dissatisfied, 2: not very satisfied, 3: satisfied and 4: very satisfied) with an accuracy of 91.96{\%}, a precision of 79.09{\%}, a Sensitivity of 75.66{\%} and a Specificity of 92.09{\%}, regarding the students' perception of their personal and social attitudes. As a result, the higher institution will be able to take measures to monitor and correct the strengths and weaknesses of each variable related to satisfaction with the quality of the educational service.},
author = {Chamorro-Atalaya, Omar and Olivares-Zegarra, Soledad and Paredes-Soria, Alejandro and Samanamud-Loyola, Oscar and Santos, Marco Anton De Los and Santos, Juan Anton De Los and Fierro-Bravo, Maritte and Villanueva-Acosta, Victor},
doi = {10.14569/IJACSA.2021.0121289},
